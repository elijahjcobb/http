  "bugs": {
    "url": "https://github.com/elijahjcobb/hydrogen/issues"
  },
  "homepage": "https://github.com/elijahjcobb/hydrogen#readme",
  "dependencies": {
    "@ejc-tsds/arraylist": "latest",
    "@ejc-tsds/dictionary": "latest",
    "@ejc-tsds/stack": "latest",
    "@elijahjcobb/prom-type": "latest",
    "typit": "latest"
  },
  "devDependencies": {
    "@types/node": "latest",
